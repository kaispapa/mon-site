<!DOCTYPE html>
<html lang="fr">
  <head>
   
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="x-ua-compatible" content="IE=9" /><![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="site CV et quelque cours">
    <meta name="author" content="Abdelatif BOUKEHIL">
    
     <title>{% block title %}Bonjour !{% endblock %}</title>

        {% block stylesheets %}
         {% stylesheets 'css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <link rel="stylesheet" type="text/css" href="{{asset('font-awesome-4.2.0/css/font-awesome.css')}}">
        <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
        {% endblock %} 
   
    <script type="text/javascript" src="{{asset('js/modernizr.custom.js')}}"></script>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
    <body>
        {% block body %}
        {% block header%}
        {% include('navmenu.html.twig')%}
        {%endblock%}

        {% block content %}
        {% endblock %}

        {% block footer %}
        {% include('footer.html.twig')%}
        {% endblock %}

        {% endblock body %}

        {% block javascripts %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
        {% javascripts 'js/*' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
            <script>
              $(function(){
                  $("#head-title").typed({
                    strings: ["Developpement Web^1000", "Developpment Logiciels^1000" ,"Formation^1000"],
                    typeSpeed: 100,
                    loop: true,
                    startDelay: 100
                  });
              });
            </script>

    <script type="text/javascript" src="js/main.js"></script>

        {% endblock %}
    </body>
</html>
